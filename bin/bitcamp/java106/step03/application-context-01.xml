		    <value type="int">1980</value>
		</constructor-arg>
    </bean>

	<bean id="c6" class="bitcamp.java106.step03.Car">
		<constructor-arg>
			<value type="int">1980</value>
		</constructor-arg>
		<constructor-arg>
			<value type="java.lang.String">소나타</value>
		</constructor-arg>
	</bean>

	<bean id="c9" class="bitcamp.java106.step03.Car">
		<constructor-arg index="0">
			<value type="java.lang.String">소나타</value>
		</constructor-arg>
		<constructor-arg index="1">
			<value type="int">1980</value>
		</constructor-arg>
    </bean>

	<bean id="c9" class="bitcamp.java106.step03.Car">
		<constructor-arg index="0">
			<value type="java.lang.String">소나타</value>
		</constructor-arg>
		<constructor-arg index="1">
			<value type="int">1980</value>
		</constructor-arg>
    </bean> 
</beans>